{
	"manifest_version": 2,
	"name": "Milkdome anti-adblock pwner",
	"version": "0.1",
	"description": "Allows to watch Milkdome streams with ad-blocker activated",
	"content_scripts": [
		{
			"js": [ "mkd-pwn.js" ],
			"matches": [ "*://beta.milkdome.com/*" ],
			"run_at": "document_start"
		}
	],
	"permissions": [ "*://beta.milkdome.com/*" ]
}
